{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/MinCut.ConsoleApp/MinCut.ConsoleApp.csproj"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "runConsole",
            "command": "dotnet",
            "args": [
                "run",
                "-p",
                "MinCut.ConsoleApp"
            ],
            "problemMatcher": [
                "$msCompile"
            ]
        }
    ]
}